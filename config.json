{
  "port": 8888,
  "db": {
    "dialect": "mysql",
    "url": "root@tcp(127.0.0.1:3306)/basecloud?charset=utf8&parseTime=True&loc=Local",
    "maxIdleCons": 10,
    "maxOpenCon": 100,
    "printLog": true
  },
  "tpl": [
    {
      "name": "index",
      "root": "vue/",
      "filename": "index.vue",
      "needModule": true
    },
    {
      "name": "add-or-update",
      "root": "vue/",
      "filename": "add-or-update.vue",
      "needModule": true
    },
    {
      "name": "store",
      "root": "vue/",
      "filename": ".js",
      "needModule": false,
      "appendFileName": true
    },
    {
      "name": "mapper",
      "root": "server/",
      "needModule": true,
      "filename": "Dao.xml",
      "appendFileName": true
    }
  ]
}